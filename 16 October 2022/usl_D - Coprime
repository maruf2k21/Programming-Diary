#include<bits/stdc++.h>
#define endl '\n'
#define ll long long int
using namespace std;

int gcd(int a,int b)
{
	return b==0 ? a: gcd(b,a%b);
}


int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int tst;
	cin>>tst;
	while(tst--)
	{
		int n,flag=0;
		cin>>n;
		int arr[n];
		vector<int>v;
		for(int i=0; i<n; i++)
		{
			cin>>arr[i];
		}
		for(int i=0; i<n; i++)
		{
			for(int j=0; j<n; j++)
			{
				if(gcd(arr[i],arr[j])==1)
				{
					v.push_back(i+j+2);
					flag=1;
				}
			}
		}
		if(flag==0)cout<<-1<<endl;
		else 
		{
			cout<<*max_element(v.begin(),v.end())<<endl;
		}



		
	}


	return 0;
}
