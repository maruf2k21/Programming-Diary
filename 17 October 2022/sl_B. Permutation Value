#include<bits/stdc++.h>
#define endl '\n'
#define ll long long int
using namespace std;


int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int tst;
	cin>>tst;
	while(tst--)
	{
		int n;
		cin>>n;
		for(int i=1; i<=n; i++)
		{
			if(i%2==1)cout<<i<<" ";
		}
		for(int i=n; i>=1; i--)
		{
			if(i%2==0)cout<<i<<" ";
		}
		cout<<endl;
	
	}


	return 0;
}
