#include<bits/stdc++.h>
#define endl '\n'
#define ll long long int
using namespace std;


int main()
{
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);

	int tst;
	cin>>tst;
	while(tst--)
	{
		int n;
		cin>>n;
		int arr[n];
		for(int i=0; i<n;i++)
		{
			cin>>arr[i];
		}
		if(10-n==2)cout<<6<<endl;
		else if(10-n==3)cout<<6*3<<endl;
		else if(10-n==4)cout<<6*6<<endl;
		else if(10-n==5)cout<<6*10<<endl;
		else if(10-n==6)cout<<6*15<<endl;
		else if(10-n==7)cout<<6*21<<endl;
		else if(10-n==8)cout<<6*28<<endl;
		else if(10-n==9)cout<<6*36<<endl;
	}


	return 0;
}
